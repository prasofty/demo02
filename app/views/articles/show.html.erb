<div class="row">
  <div class="span8">
    <div class="page-header">
      <h4 class="article_title clearfix">
        <%= link_to @article.title.titleize, article_path(@article.id)%>
        <small>By <%= @article.user.email%> - <%= @article.created_at.strftime("%b %d, %Y %H:%M") %></small>
      </h4>
    </div>
    <p class="lead">
      <%= @article.content%>
    </p>
  </div>
  <div class="span4">

  </div>
</div>




